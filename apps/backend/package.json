{
  "name": "@poll-automation/backend",
  "version": "1.0.0",
  "private": true,
  "description": "Node.js backend for poll automation, handling transcription and web application backend.",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "scripts": {
    "build": "tsc --skipLibCheck --outDir dist --rootDir src",
    "build:simple": "npm install typescript && npx tsc --project tsconfig.production.json",
    "build:fallback": "npm install -g typescript && tsc --project tsconfig.production.json",
    "start": "node dist/index.js",
    "dev": "ts-node-dev src/index.ts",
    "dev:transcription": "nodemon src/transcription/index.ts",
    "lint": "eslint src/**/* --fix",
    "format": "prettier --write src/**/*",
    "test": "jest",
    "test:client": "ts-node test-client.ts",
    "render:build": "npm run build:simple || npm run build:fallback",
    "render:start": "node dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.1",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.1",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-oauth2": "^1.8.0",
    "@types/streamifier": "^0.1.2",
    "@types/ws": "^8.18.1",
    "@types/xlsx": "^0.0.36",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "buffer": "^6.0.3",
    "cloudinary": "^2.7.0",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "google-auth-library": "^8.9.0",
    "helmet": "^8.1.0",
